{
  "baseURL": "https://mikey-666.github.io/blog/",
  "title": "Mike的博客",
  "pagination": {
    "pagerSize": 10
  },
  "theme": "PaperMod",
  "copyright": "Mike",
  "favicon": "/favicon.ico",

  "languageCode": "zh-CN",
  "hasCJKLanguage": true,
  "defaultContentLanguage": "zh-cn",
  "defaultContentLanguageInSubdir": true,
  "languages": {
    "zh-cn": {
      "languageName": "简体中文",
      "title": "Mike的博客",
      "subtitle": "欢迎来到Mike的博客",

      "imageUrl": "",
      "imageTitle": "",
      "imageWidth": 0,
      "imageHeight": 0,

      "buttons": [
        {
          "name": "",
          "url": ""
        }
      ],
      "weight": 1,
      "params": {
        "description": "Mike的博客"
      }
    }
  },

  "enableInlineShortcodes": false,
  "enableEmoji": true,
  "enableRobotsTXT": false,
  "buildDrafts": false,
  "buildFuture": false,
  "buildExpired": false,
  "pygmentsUseClasses": true,

  "minify": {
    "disableXML": true
  },

  "permalinks": {
    "post": "/:title/"
  },

  "menu": {
    "main": [
      {
        "identifier": "menu",
        "name": "Menu",
        "url": "/menu/",
        "weight": 2
      },
      {
        "identifier": "home",
        "name": "Home",
        "url": "./",
        "weight": 1
      }
    ]
  },

  "outputs": {
    "home": [
      "HTML",
      "RSS",
      "JSON"
    ]
  },

  "params": {
    "env": "production",
    "description": "Mike的博客",
    "author": [ "Mike" ]
  },

  "defaltTheme": "dark",
  "disableThemeToggle": false,
  "DateFormat": "2006-01-02",
  "ShowShareButtons": true,
  "ShowReadingTime": true,
  "displayFullLangName": true,
  "ShowPostNavLinks": true,
  "ShowBreadCrumbs": true,
  "ShowCodeCopyButton": true,
  "hideFooter": false,
  "ShowWordCounts": true,
  "VisitCount": true,

  "ShowLastMod": true,

  "ShowToc": true,
  "TocOpen": true,

  "comments": true,

  "socialIcons": [
    {
      "name": "GitHub",
      "url": "https://github.com/mikey-666"
    }
  ],

  "assets": {

  },

  "fuseOpts": {
    "isCaseSensitive": false,
    "shouldSort": true,
    "location": 0,
    "distance": 1000,
    "threshold": 1,
    "minMatchCharLength": 0,
    "keys": [ "title", "permalink", "summary" ]
  },

  "twikoo": {
    "version": "1.4.11"
  },
  "taxonomies": {
    "category": "categories",
    "tag": "tags",
    "series": "series"
  },

  "markup": {
    "defaultMarkdownHandler": "goldmark",
    "goldmark": {
      "renderer": {
        "unsafe": true
      }
    },
    "highlight": {
      "codeFences": true,
      "guessSyntax": true,
      "lineNos": true,
      "style": "github-dark"
    }
  },
  "privacy": {

  },
  "services": {

  }
}